{
  "name": "cypress_inside_track",
  "version": "1.0.0",
  "description": "UI Testing of Inside Track using Cypress",
  "main": "index.js",
  "scripts": {
    "tests": "cypress run --browser chrome --spec \"cypress/e2e/tests/*.cy.js\"",
    "tests-electron": "cypress run --spec \"cypress/e2e/tests/*.cy.js\"",
    "tests-firefox": "cypress run --browser firefox --spec \"cypress/e2e/tests/*.cy.js\"",
    "tests-edge": "cypress run --browser edge --spec \"cypress/e2e/tests/*.cy.js\"",
    "fixture-tests": "cypress run --browser chrome --spec \"cypress/e2e/tests/use_fixtures.cy.js\"",
    "negative-tests": "cypress run --browser chrome --spec \"cypress/e2e/tests/negative.cy.js\"",
    "tests-min": "cypress run --browser chrome --reporter min --spec \"cypress/e2e/tests/*.cy.js\"",
    "merge-reports": "mochawesome-merge ./mochawesome-report/*.json -o ./mochawesome-report/merged-reports.json",
    "cypress": "cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^15.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0"
  }
}
